    "dexie": "^3.2.3",
    "i18next": "^23.4.4",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.37.1",
    "monaco-vim": "^0.3.4",
    "pako": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.1.0",
    "react-icons": "^5.2.1",
    "react-joyride": "^2.5.5",
    "react-router-dom": "^6.3.0",
    "semver": "^7.7.2",
